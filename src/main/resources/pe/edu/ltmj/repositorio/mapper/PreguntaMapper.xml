<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pe.edu.ltmj.repositorio.mapper.PreguntaMapper">

    <insert id="registrarPregunta" parameterType="Pregunta" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO pregunta(texto, encuesta_id) VALUES (#{texto}, #{idEncuesta})
    </insert>
</mapper>